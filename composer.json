{
  "name": "<VendorName>/<PackageName>",
  "description": "Description from package.",
  "type": "library",
  "authors": [
    {
      "name": "Name from maintainer",
      "email": "E-mail from maintainer",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "keywords": ["keywords"],
  "require": {
    "php": ">=5.6",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "VendorName\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VendorName\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
