{
  "name": "ricardokovalski/skeleton",
  "description": "Description from package.",
  "type": "library",
  "authors": [
    {
      "name": "Ricardo Kovalski Cruz",
      "email": "ricardokovalskicruz@gmail.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "keywords": ["keywords"],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "RicardoKovalski\\Skeleton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RicardoKovalski\\Skeleton\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  }
}
